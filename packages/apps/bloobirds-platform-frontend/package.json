  "author": "Bloobirds IT",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jscpd": {
    "threshold": 0.1,
    "reporters": [
      "html",
      "console"
    ],
    "format": [
      "javascript"
    ],
    "ignore": [
      "**/message_pb.js"
    ],
    "minLines": 15,
    "maxLines": 5000,
    "absolute": true,
    "gitignore": true
  },
  "release": {
    "branches": [
      "production",
      {
        "name": "staging",
        "prerelease": "preprod"
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "issuePrefixes": [
              "BG-",
              "P21-",
              "QA-"
            ],
            "issueUrlFormat": "https://bloobirds.atlassian.net/browse/{{prefix}}{{id}}"
          },
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  }
}
