{
  "manifest_version": 3,
  "name": "Bloobirds Beta",
  "description": "Browser extension for Bloobirds integration (beta version to test new features)",
  "author": "Bloobirds Team",
  "content_scripts": [
    {
      "js": [
        "assets/index.js-loader.70f05bf9.js"
      ],
      "matches": [
        "*://*.whatsapp.com/*",
        "*://*.linkedin.com/*",
        "*://*.lightning.force.com/*",
        "*://*.localhost/*",
        "*://*.auth.bloobirds.com/*",
        "*://*.auth.dev-bloobirds.com/*",
        "*://*.crm4.dynamics.com/*"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "action": {
    "default_title": "Bloobirds Beta Extension",
    "default_popup": "src/popup/index.html",
    "default_icon": "src/beta.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "history",
    "webRequest",
    "scripting",
    "action",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://nyl.as/*",
    "*://*.nyl.as/*",
    "*://t.nylas.com/*",
    "https://mail.google.com/*"
  ],
  "icons": {
    "128": "src/beta.png"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "*://*/*"
      ],
      "resources": [
        "assets/*.js",
        "assets/*.css",
        "src/beta.png",
        "src/logo.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://*.auth.bloobirds.com/*",
        "*://*.auth.dev-bloobirds.com/*",
        "*://*.crm4.dynamics.com/*",
        "*://*.lightning.force.com/*",
        "*://*.linkedin.com/*",
        "*://*.localhost/*",
        "*://*.whatsapp.com/*"
      ],
      "resources": [
        "assets/jsx-runtime.5d392229.js",
        "assets/index.js.ff2f6a1b.js"
      ],
      "use_dynamic_url": true
    }
  ],
  "version": "3.1.3.0",
  "version_name": "3.1.3-beta.10"
}
