{
  "name": "@bloobirds-it/email",
  "version": "0.0.0",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development rollup -c=../../../rollup.config.js",
    "build:prod": "cross-env NODE_ENV=production rollup -c=../../../rollup.config.js",
    "build:release": "npm run clean && cross-env NODE_ENV=production rollup -c=../../../rollup.config.js",
    "start": "rollup -w -c=../../../rollup.config.js",
    "test": "jest",
    "lint": "npm run lint:eslint && npm run lint:types",
    "lint:eslint": "eslint \"{src,test}/**/*.ts\"",
    "lint:types": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {},
  "dependencies": {
    "@bloobirds-it/rich-text-editor": "*",
    "@bloobirds-it/calendar": "*",
    "@bloobirds-it/types": "*",
    "@bloobirds-it/utils": "*",
    "@bloobirds-it/meeting": "*",
    "@bloobirds-it/misc-modals": "*",
    "@bloobirds-it/activity-feed": "*",
    "@bloobirds-it/bobjects": "*",
    "@bloobirds-it/playbook": "*",
    "@bloobirds-it/misc": "*",
    "@bloobirds-it/banner": "*",
    "@bloobirds-it/confirm-close-modal": "*",
    "@bloobirds-it/hooks": "*",
    "@bloobirds-it/discovery-tooltips": "*"
  }
}
