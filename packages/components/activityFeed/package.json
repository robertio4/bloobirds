  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development rollup -c=../../../rollup.config.js",
    "build:prod": "cross-env NODE_ENV=production rollup -c=../../../rollup.config.js",
    "build:release": "npm run clean && cross-env NODE_ENV=production rollup -c=../../../rollup.config.js",
    "start": "rollup -w -c=../../../rollup.config.js",
    "test": "jest",
    "lint": "npm run lint:eslint && npm run lint:types",
    "lint:eslint": "eslint \"{src,test}/**/*.ts\"",
    "lint:types": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {},
  "dependencies": {
    "@bloobirds-it/copilot": "*",
    "@bloobirds-it/activity-timeline-item": "*",
    "@bloobirds-it/bobjects": "*",
    "@bloobirds-it/cadence": "*",
    "@bloobirds-it/contact-flow": "*",
    "@bloobirds-it/hooks": "*",
    "@bloobirds-it/light-attachment-list": "*",
    "@bloobirds-it/meeting": "*",
    "@bloobirds-it/rich-text-editor": "*",
    "@bloobirds-it/types": "*",
    "@bloobirds-it/utils": "*",
    "@bloobirds-it/virtual-infinite-scroll": "*",
    "ace-builds": "^1.32.3",
    "react-ace": "^10.1.0",
    "wavesurfer.js": "^6.3.0"
  }
