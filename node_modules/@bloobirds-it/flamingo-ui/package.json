{
  "name": "@bloobirds-it/flamingo-ui",
  "version": "1.221.0",
  "description": "Our react components library.",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "restricted"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "scripts": {
    "type": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest -c jest.config.js --collectCoverage --coverageReporters='text'",
    "test:watch": "jest --watch",
    "format": "prettier . --ignore-path .gitignore --check ",
    "format:fix": "prettier . --ignore-path .gitignore --write",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "docs": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 3001",
    "docs:build": "NODE_OPTIONS=--openssl-legacy-provider build-storybook -c .storybook -o storybook-static",
    "generate:component": "node ./scripts/component",
    "generate:colors": "node ./scripts/colors",
    "generate:icons": "node ./scripts/icons",
    "generate:flags": "node ./scripts/flags",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@chakra-ui/hooks": "^1.9.1",
    "@popperjs/core": "^2.9.2",
    "@react-hook/mouse-position": "^4.1.3",
