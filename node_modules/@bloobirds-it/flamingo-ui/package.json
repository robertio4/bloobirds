    "react-input-autosize": "^3.0.0",
    "react-popper": "^2.2.5",
    "react-resize-detector": "^6.7.3",
    "react-textarea-autosize": "^8.3.3",
    "react-use": "^17.2.4",
    "recharts": "2.9.0",
    "use-sync-external-store": "^1.2.0",
    "spacetime": "7.4.7"
