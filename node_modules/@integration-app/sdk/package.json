{
  "name": "@integration-app/sdk",
  "version": "0.16.18",
  "description": "JavaScript SDK for Integration.app",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:compile && npm run build:bundle",
    "clean": "find . -type f -name \"*.js\" -o -name \"*.d.ts\" -not -path \"./node_modules/*\" -not -name \"rollup.config.js\" -not -name \".eslintrc.js\" -delete",
    "build:compile": "tsc --project tsconfig.build.json",
    "build:bundle": "rollup -c rollup.config.js",
    "docs": "typedoc --tsconfig tsconfig.json",
    "docs:dev": "typedoc --tsconfig tsconfig.json --watch",
    "dev": "tsc --watch",
    "lint": "eslint -c .eslintrc.js src",
    "test": "TZ=UTC jest",
    "test:watch": "jest --watch",
    "version": "npm version"
  },
  "devDependencies": {
    "@types/insert-css": "^2.0.0",
    "@types/jest": "^27.0.2",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.3.1",
    "rollup": "^2.52.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.2.0",
    "tsconfig-paths": "^3.10.1",
    "typedoc": "^0.23.21",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "~4.8.4"
  },
  "author": "integration.app",
  "dependencies": {
    "@sinclair/typebox": "^0.23.4",
    "@ungap/structured-clone": "^1.0.1",
    "axios": "^0.21.4",
    "fast-deep-equal": "^3.1.3",
    "humanparser": "^2.6.0",
    "insert-css": "^2.0.0",
    "js-convert-case": "^4.2.0",
    "lodash": "^4.17.21",
    "penpal": "^6.2.1",
    "phone": "^3.1.22",
    "pusher-js": "^7.0.3",
    "query-string": "^7.1.1",
    "tslib": "^2.3.1",
    "url-join": "^4.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "json",
      "js"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
